<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.android.lxf"
          xmlns:android="http://schemas.android.com/apk/res/android">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:name=".test.AppApplacation"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".test.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".test.SubMessageActivity"
            android:label="@string/title_activity_base">
        </activity>
        <activity
            android:name=".test.HttpActivity"
            android:label="@string/title_activity_http">
        </activity>
        <activity
            android:name=".test.MessageActivity"
            android:label="@string/title_activity_message">
        </activity>
        <activity
            android:name=".test.DbActivity"
            android:label="@string/title_activity_db">
        </activity>
    </application>

</manifest>
