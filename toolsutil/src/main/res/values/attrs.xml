<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RotateLoading">
        <attr name="loading_color" format="color" />
        <attr name="loading_width" format="dimension" />
        <attr name="shadow_position" format="dimension" />
    </declare-styleable>
    <declare-styleable name="slideswitch">
        <attr name="themeColor" format="color" />
        <attr name="isOpen" format="boolean" />
        <attr name="lefttext" format="reference" />
        <attr name="righttext" format="reference" />
        <attr name="shape_sw">
            <enum name="rect" value="1" />
            <enum name="circle" value="2" />
        </attr>
    </declare-styleable>
</resources>